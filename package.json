{
  "name": "secret-forklift",
  "version": "0.1.1",
  "description": "AWS Parameter Store loader for Node.js. Load secrets, tokens, and application configuration variables from AWS Systems Manager's Parameter Store into your Node.js app!",
  "keywords": [
    "aws",
    "ssm",
    "parameter store",
    "secret",
    "env"
  ],
  "homepage": "https://bitbucket.org/omnio-interactive/secret-forklift",
  "bugs": {
    "url": "https://bitbucket.org/omnio-interactive/secret-forklift/jira"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Luke Carr",
      "email": "luke@omniointeractive.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/omnio-interactive/node-secret-forklift.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "aws-param-store": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.9.1",
    "aws-sdk": "^2.640.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
